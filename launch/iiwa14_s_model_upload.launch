<?xml version="1.0"?>
<launch>
  <arg name="hardware_interface" default="PositionJointInterface"/> 
  <arg name="robot_name" default="iiwa"/>
  <group ns="$(arg robot_name)" >
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find iiwa14_s_model)/urdf/iiwa14_touch_s_model.urdf.xacro' hardware_interface:=$(arg hardware_interface) robot_name:=$(arg robot_name)" />
  </group>

</launch>
